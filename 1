---
- name: This is a Windows post-provisioning Playbook
  hosts: all
  gather_facts: false
  tasks:
    - name: Local Group creation
      win_group:
        name: hello-world
        force: true
        gid: 786
        #        state: absent
      register: result

    - name: get local group creation details
      debug: 
        msg: result
        verbosity: 4
